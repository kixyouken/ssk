{
    "name": "用户",
    "table": {
        "name": "users",
        "joins": [
            {
                "name": "master_university",
                "foreign": "id",
                "key": "master_university_id",
                "join": "inner"
            },
            {
                "name": "master_job",
                "foreign": "id",
                "key": "master_job_id",
                "join": "inner"
            }
        ],
        "deleted": {
            "field": "deleted_at",
            "value": ""
        }
    },
    "columns": [
        {
            "label": "ID",
            "field": "id"
        },
        {
            "label": "姓名",
            "field": "name"
        },
        {
            "label": "邮箱",
            "field": "email"
        },
        {
            "label": "生日",
            "field": "birthday"
        },
        {
            "label": "学校名称",
            "field": "master_university.name AS university_name"
        },
        {
            "label": "職種名称",
            "field": "master_job.name AS job_name"
        }
    ]
}