{
    "name": "用户",
    "table": {
        "name": "users",
        "joins": [
            {
                "name": "master_university",
                "foreign": "id",
                "key": "master_university_id",
                "join": "inner",
                "wheres": [
                    {
                        "field": "id",
                        "search": "eq",
                        "value": "2"
                    },
                    {
                        "field": "name",
                        "search": "eq",
                        "value": "明治大学"
                    }
                ],
                "columns": [
                    {
                        "label": "大学名称",
                        "field": "name"
                    }
                ]
            },
            {
                "name": "master_job",
                "foreign": "id",
                "key": "master_job_id",
                "join": "inner"
            }
        ],
        "withs": [
            {
                "name": "users",
                "foreign": "master_university_id",
                "key": "id",
                "join": "inner",
                "columns": [
                    {
                        "label": "用户ID",
                        "field": "id"
                    },
                    {
                        "label": "用户名称",
                        "field": "name"
                    }
                ],
                "orders": [
                    {
                        "field": "id",
                        "sort": "desc"
                    }
                ]
            }
        ],
        "withs_count": [
            {
                "name": "users",
                "foreign": "master_university_id",
                "key": "id",
                "join": "inner"
            }
        ]
    },
    "columns": [
        {
            "label": "ID",
            "field": "id"
        },
        {
            "label": "姓名",
            "field": "name"
        },
        {
            "label": "邮箱",
            "field": "email"
        },
        {
            "label": "生日",
            "field": "birthday"
        },
        {
            "label": "状态",
            "field": "status",
            "attr": [
                {
                    "in": "0",
                    "out": "未入力"
                },
                {
                    "in": "1",
                    "out": "内定"
                },
                {
                    "in": "2",
                    "out": "重複"
                },
                {
                    "in": "3",
                    "out": "公務員"
                },
                {
                    "in": "4",
                    "out": "教職"
                },
                {
                    "in": "5",
                    "out": "既卒"
                },
                {
                    "in": "6",
                    "out": "卒年延長"
                },
                {
                    "in": "7",
                    "out": "留学"
                },
                {
                    "in": "8",
                    "out": "進学"
                },
                {
                    "in": "9",
                    "out": "ダミー登録"
                },
                {
                    "in": "91",
                    "out": "メールエラー"
                }
            ]
        }
    ]
}